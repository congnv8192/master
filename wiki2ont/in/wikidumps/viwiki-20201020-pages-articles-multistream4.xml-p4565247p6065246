  <page>
    <title>Thích Chánh Quả</title>
    <ns>0</ns>
    <id>4795407</id>
    <revision>
      <id>41374997</id>
      <parentid>30726687</parentid>
      <timestamp>2018-07-29T23:45:31Z</timestamp>
      <contributor>
        <username>ThitxongkhoiAWB</username>
